<!DOCTYPE html>
<html>
  <head>
    <title>Xtasy2020</title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
    <meta charset="UTF-8" />
		<meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
    <script src="https://code.jquery.com/jquery-3.2.1.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/sweetalert2@9"></script>
  </head>
  <body>
  <%- include('partials/preloader') %>
  <%- include('partials/loginbutton') %>
<section >
      <div class="title-div">
        <div class="img-fluid title-img-div" >
          <img src="images/xtasy2020white.png" class="title-img">
        </div>
      </div>

      <div style="height: 30vh;">
        <%- include('partials/timer') %>
      </div>
      <center>
        <%- include('partials/navbar') %>
      </center>
    </section>
    <% if(message) { %>
      <script src="js/sweet.js"></script>
      <script>
        let message = "<%= message %>";
        Toast.fire({
          title: message
        });
      </script>
      <script>
        history.pushState(null, null, window.location.pathname);
      </script>
    <% } %>
    <% if(registered) { %>
      <div class="register">
        <script>
          Swal.fire({
            position: 'top',
            icon: 'success',
            title: 'You have successfully registered for Xtasy 2020.',
            showConfirmButton: false,
            timer: 7000
          })
          </script>
          
        </div>
      <% } %>
